# Courtesy of a github user that I cannot remember the name of that made a comment on some issue
def_key "t"
    find
def_key "t"
    find_item_forward
def_key "+"
    show_clock
def_key "="
    volume_up
## The most important part
def_key "j"
    scroll_down
def_key "k"
    scroll_up
def_key "h"
    previous_column
def_key "l"
    next_column
## End of most important part
def_key "."
    show_lyrics
def_key "n"
    next_found_item
def_key "N"
    previous_found_item
def_key "J"
    move_sort_order_down
def_key "K"
    move_sort_order_up
def_key "ctrl-u"
    page_up
def_key "ctrl-d"
    page_down
